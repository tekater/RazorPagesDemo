@page
@model RazorPagesDemo.Pages.EmployeeModel
@{
    ViewData["Title"] = "GetEmployee Page";
}
<div>
    <table>
    <tr>
            @for (int i = 0; i < Model.EmployeeList.Count; i++)
            {
                <td>
                    <label>
                        Name: @Model.EmployeeList[i].FirstName <br/>
                </label>

                    <label>
                        Last Name: @Model.EmployeeList[i].LastName 
                    </label>

                    <label>
                        Birth: @Model.EmployeeList[i].Birth
                    </label>
                    </td>
            }
    </tr>
</table>
</div>
